<div class="row wrapper border-bottom white-bg page-heading" style="">
    <!-- ==========Start content=========== -->
    <div class="col-lg-10">
        <h2>
            Change Password
        </h2>
        <ol class="breadcrumb">
            <li>
                <a>
                    Dashboard
                </a>
            </li>
            <li class="active">
                <strong >
                    Change Password
                </strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">
    </div>
</div>
<%if(message != ''){%>
<div class="col-lg-12 alert alert-<%=type_message%>" style="margin-top: 5px;">
     <%=message%>
  </div>
<%}%>
<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        Change Password
                    </h5>
                </div>
                <div class="ibox-content">
                    <form method="POST" class="form-horizontal" action="/cpanel/member/changepwd">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">
                                Password
                            </label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" value="" name="pwd">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">
                                Retype Password
                            </label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" value="" name="rtpwd">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4 col-sm-offset-2">
                                <button class="btn btn-primary " type="submit">
                                    <i class="fa fa-plus">
                                    </i>
                                    &nbsp;Change Password
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
